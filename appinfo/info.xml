	<id>profiler</id>
	<name>Profiler</name>
	<summary>Profiler for nextcloud</summary>
	<description><![CDATA[Provides a profiler for Nextcloud. Making developing fast apps easier.]]></description>
	<version>4.0.0-dev.0</version>
	<licence>agpl</licence>
	<author>Carl Schwan</author>
	<namespace>Profiler</namespace>

	<types>
		<logging/>
	</types>

	<category>customization</category>

	<website>https://github.com/nextcloud/profiler</website>
	<bugs>https://github.com/nextcloud/profiler/issues</bugs>
	<repository>https://github.com/nextcloud/profiler.git</repository>

	<dependencies>
		<nextcloud min-version="33" max-version="33"/>
	</dependencies>

	<commands>
		<command>OCA\Profiler\Command\Enable</command>
		<command>OCA\Profiler\Command\Disable</command>
		<command>OCA\Profiler\Command\ListCommand</command>
		<command>OCA\Profiler\Command\Export</command>
		<command>OCA\Profiler\Command\Clear</command>
		<command>OCA\Profiler\Command\Compare</command>
	</commands>
</info>
